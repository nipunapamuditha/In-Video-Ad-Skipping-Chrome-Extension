{
  "manifest_version": 3,
  "name": "skip in video ads",
  "version": "1.1",
  "description": "Skips the in video ads/sponsored segments of any YouTube video.",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "yoyo.png",
    "48": "yoyo.png",
    "128": "yoyo.png"
  }
}